
!!! ====================================

!Xft.dpi: 192
!Xft.dpi: 180
!Xft.dpi: 168
!Xft.dpi: 144
!Xft.dpi: 120
!Xft.dpi: 112
Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!Xcursor.size: 16

!!! ====================================

!! ----  Nord -------
!rofi.color-enabled: true
!rofi.color-window: #2E3440, #8EBBBA, #3A4151
!rofi.color-normal: #2E3440, #8EBBBA, #2E3440, #2E3440, #ffffff
!rofi.color-active: #2E3440, #8EBBBA, #2E3440, #2E3440, #ffffff
!rofi.color-urgent: #2E3440, #8EBBBA, #2E3440, #2E3440, #ffffff

!! ----  Win95 -------
rofi.color-enabled: true
rofi.color-window: #000000, #000080, #000080
rofi.color-normal: #000000, #71747C, #000000, #000000, #ffffff
rofi.color-active: #000000, #71747C, #000000, #000000, #ffffff
rofi.color-urgent: #000000, #71747C, #000000, #000000, #ffffff

!! Separator style (none, dash, solid)
rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
!rofi.font: TerminessTTF Nerd Font Mono 10.5
!rofi.font: Source Code Pro 10.5
!rofi.font: BitstreamVeraSansMono Nerd Font Mono 10.5
rofi.font: Terminus 10
!rofi.font: Terminus 8
rofi.bw: 1
rofi.columns: 3
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true
rofi.width: 40

!!! ====================================

XTerm*background:        #222D31
XTerm*foreground:        #d8d8d8
XTerm*pointerColor:      #1ABB9B
!XTerm*faceName:          Fixed
!XTerm*faceSize:          11
XTerm*reverseVideo:      on
XTerm*selectToClipboard: true

